package tanks.game;

import java.awt.*;

import javax.swing.*;

import tanks.game.display.Display;

public class Main{

	public static void main(String[] args){
		Display.create(1200, 800, "Tanks-GAME");

		Timer timer = new Timer(1000 / 60, new event.AbstractAction(){
			public void actionPerformed(event.ActionEvent e){

			}
		});
		timer.setRepeats(true);
		timer.start();
	}

}
