package tanks.game.textures;

import java.awt.image.BufferedImage;

public class ListPictures {

	private BufferedImage pictureList;
	private int countPicture;
	private int sizePicture;
	private int pictureWidth;
	public ListPictures(BufferedImage pictureList, int countPicture,
			int sizePicture) {
		this.pictureList = pictureList;
		this.countPicture = countPicture;
		this.sizePicture = sizePicture;
	}

}
